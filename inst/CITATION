citHeader("To cite Icarus in publications use:")

meta <- packageDescription("Icarus")
year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)
url <- meta$URL

citEntry(entry="Manual",
	title="{Icarus}: an R package for calibration in survey sampling",	
	author=personList(as.person("Rebecq A.")),
	year=year,
	note=note,
	url=url,
	textVersion = paste0("Rebecq, A. ",year,". Icarus: an R package for calibration in survey sampling.",note,". URL ",url,".")
)


